{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://practical-becky-chattrix-f0677ddd.koyeb.app/api/$1"
    },
    {
      "source": "/health",
      "destination": "https://practical-becky-chattrix-f0677ddd.koyeb.app/health"
    },
    {
      "source": "/media/(.*)",
      "destination": "https://res.cloudinary.com/dcwkrjzww/$1"
    },
    {
      "source": "/static/(.*)",
      "destination": "https://practical-becky-chattrix-f0677ddd.koyeb.app/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Accel-Buffering",
          "value": "no"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}